<% layout('layouts/boilerplate') -%> 

<table class="table table-bordered table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col" class="col-3">Card</th>
            <th scope="col" class="col-1">#</th>
            <th scope="col" class="col-1">Rarity</th>
            <th scope="col" class="col-7">Count</th>
        </tr>
    </thead>
    <tbody>
        <% for(let card of cards) { %>
            <%- include('./item', {card : card, set: theSet}); %>

                <% } %>
    </tbody>
</table>


